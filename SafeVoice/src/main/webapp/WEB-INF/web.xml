<?xml version="1.0" encoding="EUC-KR"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

<!-- Login.jsp mapping -->

<servlet>
  <servlet-name>LoginPage</servlet-name>
  <jsp-file>/WEB-INF/Login.jsp</jsp-file>
</servlet>
<servlet-mapping>
  <servlet-name>LoginPage</servlet-name>
  <url-pattern>/Login.jsp</url-pattern>
</servlet-mapping>

<!-- SignIn.jsp mapping -->
<!-- SignIn.jsp 매핑 -->
<servlet>
  <servlet-name>SignInPage</servlet-name>
  <jsp-file>/WEB-INF/SignIn.jsp</jsp-file>
</servlet>
<servlet-mapping>
  <servlet-name>SignInPage</servlet-name>
  <url-pattern>/SignIn.jsp</url-pattern>
</servlet-mapping>


<!-- Join.jsp mapping -->
<servlet>
  <servlet-name>JoinPage</servlet-name>
  <jsp-file>/WEB-INF/Join.jsp</jsp-file>
</servlet>
<servlet-mapping>
  <servlet-name>JoinPage</servlet-name>
  <url-pattern>/join</url-pattern>
</servlet-mapping>


</web-app>
